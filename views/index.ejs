<title><%- title %></title>
<script type="text/javascript" src="/extjs/ext-all.js"></script>
<script type="text/javascript" src="/extjs/build/ext-theme-classic.js"></script>
<script type="text/javascript" src="/socket.io/socket.io.js"></script>
<script type="text/javascript">
        var socket = io();
        var user = {};
</script>
<% if(user){ %>
    <script type="text/javascript">
            user.id = '<%- user.id %>';
            user.first_name = '<%- user.first_name %>';
            user.last_name = '<%- user.last_name %>';
            user.access_level = '<%- user.access_level %>';
            user.email = '<%- user.email %>';
    </script>
<% }else{ %>
    <script type="text/javascript">
            user.id = '1';
            user.first_name = '2';
            user.last_name = '3';
            user.access_level = '0';
            user.email = '5';
    </script>
<% } %>
<% var menu_data= "Ext.Loader.setConfig({"
        +" enabled: true,"
        +"  disableCaching: true"
        +"});"
        +"Ext.require(["
        +"'Ext.panel.Panel',"
        +"'Ext.window.MessageBox',"
        +"'Ext.chart.*'"
        +"]);"
        +"Ext.onReady(function() {"
        +" var onClick = function(menuItem) {"
        +" Ext.Msg.show({"
        +"  title: 'Click!',"
        +"   msg: 'You clicked item',"
        +"   icon: Ext.Msg.INFO,"
        +"   buttons: Ext.Msg.OK"
        +"   });"
        +" };"
        +"  Ext.create('Ext.container.Viewport', {"
        +"  layout: 'border',"
        +"  items: ["
        +"  header_panel, navigation_panel, tab_panel, footer_panel ]"
        +"   });"
        +"})";
  %>
